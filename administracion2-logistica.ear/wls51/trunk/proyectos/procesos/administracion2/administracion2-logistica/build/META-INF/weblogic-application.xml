<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-application xmlns:wls="http://xmlns.oracle.com/weblogic/weblogic-application" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_5.xsd http://xmlns.oracle.com/weblogic/weblogic-application http://xmlns.oracle.com/weblogic/weblogic-application/1.1/weblogic-application.xsd">
    
    <!--weblogic-version:10.3.3-->
    <wls:application-param>
        <wls:param-name>webapp.encoding.default</wls:param-name>
        <wls:param-value>UTF-8</wls:param-value>
    </wls:application-param>
    
    <!-- Libreria compartida del framework -->
    <wls:library-ref>
		<wls:library-name>tecnologia2-framework2-sharedlib</wls:library-name>
		<wls:specification-version>2.0</wls:specification-version>
		<wls:implementation-version>2.1.0</wls:implementation-version>
	</wls:library-ref>
	
	<!-- Libreria compartida de seguridad del framework -->
    <wls:library-ref>
		<wls:library-name>tecnologia2-seguridad-sharedlib</wls:library-name>
		<wls:specification-version>1.0</wls:specification-version>
		<wls:implementation-version>1.1.0</wls:implementation-version>
	</wls:library-ref>
	
	<!-- Libreria compartida de terceros (spring, ibatis, etc, etc) -->
    <wls:library-ref>
		<wls:library-name>tecnologia-terceros-sharedlib</wls:library-name>
		<wls:specification-version>1.0</wls:specification-version>
		<wls:implementation-version>1.0.6</wls:implementation-version>
	</wls:library-ref>
	
	<!-- Libreria compartida transversal del SIGA -->
	<wls:library-ref>
		<wls:library-name>recurso2-administracion-siga-sharedlib</wls:library-name>
		<wls:specification-version>1.2</wls:specification-version>
		<wls:implementation-version>1.6.0</wls:implementation-version>
	</wls:library-ref>
		
</wls:weblogic-application>